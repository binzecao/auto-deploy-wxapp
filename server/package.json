{
  "name": "auto-deploy-wxapp-server",
  "version": "1.0.0",
  "description": "A websocket server for auto deploy the YunZhi wxapp to the weixin editor",
  "main": "index.js",
  "bin": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "node ./index.js",
    "test": "mocha --report spec --recursive ./test/unit-test/*.js",
    "pkg": "pkg . --out-path=../dist/server/"
  },
  "author": "zebin",
  "license": "Apache-2.0",
  "pkg": {
    "scripts": [
      "./lib/*.js",
      "./*.js"
    ],
    "assets": [],
    "targets": [
      "node8-win-x64"
    ]
  },
  "devDependencies": {
    "mocha": "^5.0.5",
    "should": "^13.2.1"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "decompress": "^4.2.0",
    "iconv-lite": "^0.4.19",
    "socket.io": "^2.1.0",
    "validate": "^4.1.1",
    "winston": "^2.4.1",
    "winston-daily-rotate-file": "^3.0.1"
  }
}